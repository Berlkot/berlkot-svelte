{
  "name": "keychaincat-svelte",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bunx --bun vite dev",
    "build": "bunx --bun vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "marked": "^15.0.8",
    "sharp": "^0.34.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.24.0",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.27.0",
    "svelte-adapter-bun": "git+https://github.com/gornostay25/svelte-adapter-bun.git",
    "svelte-check": "^4.1.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.0"
  },
  "trustedDependencies": [
    "@prisma/client",
    "esbuild",
    "sharp",
    "svelte-adapter-bun"
  ]
}
